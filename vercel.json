{
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|svg|ico)",
      "dest": "/$1"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "^/$",
      "dest": "/login.html"
    }
  ]
}
